{
    "name": "automad",
    "displayName": "Automad Template Language",
    "description": "Syntax highlighting and snippets for the Automad template language",
    "version": "1.0.1",
    "license": "MIT",
    "publisher": "MarcAntonDahmen",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "grammars": [
            {
                "scopeName": "text.html.automad",
                "path": "./syntaxes/Automad.tmLanguage",
                "injectTo": [
                    "text.html"
                ]
            }
        ],
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "icon": "icon.png"
}
